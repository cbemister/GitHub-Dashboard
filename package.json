    "@types/node": "^22.0.0",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.0.0",
    "drizzle-kit": "^0.31.8",
    "electron": "^33.0.0",
